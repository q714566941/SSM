public class StudentDaoImpl implements IStudentDao {
	private SqlSession sqlSession;

	@Override
	public void insertStudent(Student student) {
		try {
			sqlSession = MyBatisUtils.getSqlSession();
			sqlSession.insert("insertStudent", student);          
			sqlSession.commit();                                  
		}finally {
			if(sqlSession != null){
				sqlSession.close();                              
			}
		}
	}


